    <head>
        <meta charset="utf-8" />
        @*<link rel="shortcut icon" type="image/x-icon" href="/Content/images/icons/favicon-updated.ico" />
        <link rel="apple-touch-icon" href="/Content/images/icons/AppleIcon-Updated.png" />*@
        <meta name="HandheldFriendly" content="true" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">
        @Styles.Render("~/Application-Original/css/bootstrap.min.css")
        @Styles.Render("~/Application-Original/js/loading-bar/loading-bar.min.css")
        @Styles.Render("~/Application-Original/css/app.css")
        <title>Decipher Austin</title>
    </head>
    <body>
