        @Scripts.Render("~/Application-Original/js/angular-simple-logger.js")
        @Scripts.Render("~/Application-Original/js/relative-paths-in-partial.js")
        @Scripts.Render("~/Application-Original/js/angular-sanitize.min.js")
        @Scripts.Render("~/Application-Original/js/angular-google-maps.min.js")
        @Scripts.Render("~/Application-Original/js/angulartics/angulartics.js")
        @Scripts.Render("~/Application-Original/js/angulartics/angulartics-ga.js")
        @Scripts.Render("~/Application-Original/js/ui-bootstrap-tpls-2.5.0.min.js")
        @Scripts.Render("~/Application-Original/client/scripts/app/config.js")
        @Scripts.Render("~/Application-Original/client/scripts/app/routing.js")
        @Scripts.Render("~/Application-Original/client/scripts/services/root.js")
        @Scripts.Render("~/Application-Original/client/scripts/services/objectHandler.js")
        @Scripts.Render("~/Application-Original/client/scripts/services/device.js")
        @Scripts.Render("~/Application-Original/client/scripts/services/db.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/common.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/home.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/user.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/find.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/review.js")
        @Scripts.Render("~/Application-Original/client/scripts/controllers/data.js")
        <script type="text/javascript">
            amplify.store("DeviceType", "Web");
		    amplify.store("AppVersion", "0.1.1-Web");
        </script>
