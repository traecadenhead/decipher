            <div ng-include src="'/Application-Original/client/views/common/header.html'"></div>
            <div class="container body-content">
                <div ui-view="mainContainer" class="mainContainer jumbotron"></div>
                <div ng-include src="'/Application-Original/client/views/common/footer.html'"></div>
            </div>
        </div>
        @Scripts.Render("~/Application-Original/js/jquery-1.12.4.min.js")
        <script type="text/javascript">
            jQuery.event.props.push('dataTransfer');
        </script>
        @*Amplify is used to store session data throughout the app*@
        @Scripts.Render("~/Application-Original/js/amplify.min.js")
        @Scripts.Render("~/Application-Original/js/injections.js")
        @Scripts.Render("~/Application-Original/js/lodash.js")
        @Scripts.Render("~/Application-Original/js/angular.min.js")
        @Scripts.Render("~/Application-Original/js/angular-ui-router.min.js")
        @Scripts.Render("~/Application-Original/js/loading-bar/loading-bar.min.js")
